{
    "manifest_version": 3,
    "name": "ブログ本文取得",
    "version": "1.0",
    "permissions": ["activeTab", "notifications", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}
